ARRIS-MUX-BNC-MIB DEFINITIONS ::= BEGIN

IMPORTS
  OBJECT-TYPE, MODULE-IDENTITY, NOTIFICATION-TYPE, Gauge32, Integer32,
  IpAddress, TimeTicks, mgmt
    FROM SNMPv2-SMI
  OBJECT-GROUP, NOTIFICATION-GROUP FROM SNMPv2-CONF
  DisplayString, DateAndTime, TruthValue, RowStatus
    FROM SNMPv2-TC;

arrisMuxBNC  MODULE-IDENTITY
   LAST-UPDATED "202005150000Z"  
   ORGANIZATION "CommScope"
   CONTACT-INFO
   "CommScope Technical Assistance Center (TAC) provides access to technicians
    24 hours a day, 7 days a week for all products.  To open a case, use
    one of the following methods:
    - E-mail at tac.helpdesk@commscope.com
    - Telephone in North America at 888-944-HELP (888-944-4357)
    - Telephone outside of North America at IDD (International Direct Dialing)
      number +1-215-323-2345
    - For Spanish language support, telephone at 215-323-2346"
   DESCRIPTION  "An SMIv2 MIB module for the CommScope Mux BNC 4.x product."
   REVISION "202005150000Z"
   DESCRIPTION
     "- Added tme2020 as an enumerated network element type in bcnElementType and
        corrected typographical errors in non-normative text."
   ::= { iso org(3) dod(6) internet(1) private(4) enterprises(1) gi(1166) giproducts(1) 102 }

   mib-2                  OBJECT IDENTIFIER ::= { mgmt 1 }
   system                 OBJECT IDENTIFIER ::= { mib-2 1 }
   bncSystem              OBJECT IDENTIFIER ::= { arrisMuxBNC 1 }
   bncSysTraps            OBJECT IDENTIFIER ::= { bncSystem 100 }  
   arrisMuxBncConformance OBJECT IDENTIFIER ::= { arrisMuxBNC 101 }
   bcs                    OBJECT IDENTIFIER ::= { iso 3 6 1 4 1 1166 7 } 
   bcsIdent               OBJECT IDENTIFIER ::= { bcs 1 }
   modSysTraps            OBJECT IDENTIFIER ::= { bcs 3 }
   modSysTrapElements     OBJECT IDENTIFIER ::= { modSysTraps 1 }
   modSysTrapControl      OBJECT IDENTIFIER ::= { modSysTraps 2 }

