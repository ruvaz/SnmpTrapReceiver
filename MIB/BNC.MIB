   bncIdNumber OBJECT-TYPE
     SYNTAX      INTEGER
     MAX-ACCESS  read-only
     STATUS      current
     DESCRIPTION "This is the BNC ID number allocated to the BNC by the license
      key.  For an unlicensed BNC application (e.g.; BNC Lite), the agent shall
      return a value of 1." 
     ::= { bncSystem 1 }

   bncPlatformHealthStatus OBJECT-TYPE
     SYNTAX      INTEGER { ok(0), unknown(1), warning(2), error(3), fatal(4) }
     MAX-ACCESS  read-only
     STATUS      current
     DESCRIPTION "The health of the hardware platform on which the BNC server is
      running as reported by the BNC platform status monitoring functionality.
      The reported value is that of the highest severity alarm status.
      Alternative mappings include Healthy->ok(0), NonCritical->warning(2) and
      Critical->fatal(4)."
     ::= { bncSystem 2 }

   bncPlatformHealthStatusLastChange OBJECT-TYPE
     SYNTAX      DateAndTime
     MAX-ACCESS  read-only
     STATUS      current
     DESCRIPTION "The timestamp of when the platform health status last 
      changed."
     ::= { bncSystem 3 }

   BncGroupEntry ::= SEQUENCE {
     bncGroupIndex                  INTEGER,
     bncGroupName                   DisplayString,
     bncGroupHealthStatus           INTEGER,
     bncGroupHealthStatusLastChange DateAndTime }

   bncGroupTable OBJECT-TYPE
     SYNTAX      SEQUENCE OF BncGroupEntry
     MAX-ACCESS  not-accessible
     STATUS      current
     DESCRIPTION "A table of the network element groups under the control of the
      BNC."
     ::= { bncSystem 51 }

   bncGroupEntry OBJECT-TYPE
     SYNTAX      BncGroupEntry
     MAX-ACCESS  not-accessible
     STATUS      current
     DESCRIPTION "A row in bncGroupTable."
     INDEX { bncGroupIndex } 
     ::= { bncGroupTable 1 }

   bncGroupIndex OBJECT-TYPE
     SYNTAX      INTEGER(1..2147483647)
     MAX-ACCESS  not-accessible
     STATUS      current
     DESCRIPTION "The network element group ID, which is used as an index into
      the table of network element groups under control of the BNC. The network
      element group ID is automatically assigned during group creation. A BNC
      server can control more than one network element group."
     ::= { bncGroupEntry 2 }

   bncGroupName OBJECT-TYPE
     SYNTAX      DisplayString
     MAX-ACCESS  read-only
     STATUS      current
     DESCRIPTION "The unique user-assigned name for the network element group."
     ::= { bncGroupEntry 3 }

   bncGroupHealthStatus OBJECT-TYPE
     SYNTAX      INTEGER { ok(0), unknown(1), warning(2), error(3), fatal(4) }
     MAX-ACCESS  read-only
     STATUS      current
     DESCRIPTION "Status of this network element group.  At startup, this 
      bncGroupHealthStatus defaults to 'unknown' (1). The bncGroupHealthStatus
      reflects the highest health status value of all network elements within
      the network element group."
     ::= { bncGroupEntry 4 }

   bncGroupHealthStatusLastChange OBJECT-TYPE
     SYNTAX      DateAndTime
     MAX-ACCESS  read-only
     STATUS      current
     DESCRIPTION "Time the last modification was made to this BNC network
      element group."
     ::= { bncGroupEntry 5 }

   bncElementTable OBJECT-TYPE
     SYNTAX      SEQUENCE OF BncElementEntry
     MAX-ACCESS  not-accessible
     STATUS      current
     DESCRIPTION "A table of the network elements under control of the BNC."
     ::= { bncSystem 52 }

   bncElementEntry OBJECT-TYPE
     SYNTAX      BncElementEntry
     MAX-ACCESS	 not-accessible
     STATUS      current
     DESCRIPTION "A row in BncElementTable."
     INDEX       { bncGroupIndexE, bncElementIndex } 
     ::=  { bncElementTable 1 }

   BncElementEntry ::= SEQUENCE {
     bncGroupIndexE                   INTEGER,
     bncElementIndex                  INTEGER,
     bncElementName                   DisplayString,
     bncElementIpAddress              IpAddress,
     bncElementType                   INTEGER,
     bncElementRedundancyGroupID      INTEGER,
     bncElementRedundancyGroupName    DisplayString,
     bncElementRedundancyRole         INTEGER,
     bncElementRequestedOpState       INTEGER,
     bncElementActualOpState          INTEGER,
     bncElementHealthStatus           INTEGER,
     bncElementHealthStatusLastChange DateAndTime,
     bncElementPositionID             DisplayString }

   bncGroupIndexE OBJECT-TYPE
     SYNTAX      INTEGER(1..2147483647)
     MAX-ACCESS  not-accessible
     STATUS      current
     DESCRIPTION "Value of bncGroupIndex associated with the network element."
     ::= { bncElementEntry 2 }

   bncElementIndex OBJECT-TYPE
     SYNTAX      INTEGER(1..2147483647)
     MAX-ACCESS  not-accessible
     STATUS      current
     DESCRIPTION "The network element ID number which is an index into the
      table of network elements under the control of the BNC. The network
      element ID number is automatically assigned during network element
      creation."
     ::= { bncElementEntry 3 }

   bncElementName OBJECT-TYPE
     SYNTAX       DisplayString
     MAX-ACCESS   read-only
     STATUS       current
     DESCRIPTION "Unique user-assigned name for the network element."
     ::= { bncElementEntry 4 }

   bncElementIpAddress OBJECT-TYPE
     SYNTAX       IpAddress
     MAX-ACCESS   read-only
     STATUS       current
     DESCRIPTION "The IP address of the network element."
     ::= { bncElementEntry 5 }

   bncElementType OBJECT-TYPE
     SYNTAX      INTEGER { se1010(0), se2000(1), tmx(2), dataServer(3),
                           adServer(4),sem(5), opRouter(6), ipRouter(7),
                           se4010(8), se4100(9), se5010(10), se5100(11),
                           modulator(12), se3000(13), se6000-6601 (14),
                           sencoreModulator(15), rE-FPM(16), fPM(17), dEM(18),
                           nTPserver(19), networkSwitch(20), inputSource(21),
                           me7000(22), se7100(23), newtec-m6100(24),
                           newtec-uss0202(25), tme2020(26) }
     MAX-ACCESS  read-only
     STATUS      current
     DESCRIPTION "Type of network element."
     ::= { bncElementEntry 6 }

   bncElementRedundancyGroupID OBJECT-TYPE
     SYNTAX      INTEGER
     MAX-ACCESS  read-only
     STATUS      current
     DESCRIPTION "The ID of the redundancy group that this network element
      belongs to. A value of (-1) indicates that the network element has not
      been placed in a redundant configuration."
     ::= { bncElementEntry 7 }

   bncElementRedundancyGroupName OBJECT-TYPE
     SYNTAX      DisplayString
     MAX-ACCESS  read-only
     STATUS      current
     DESCRIPTION "The name of the redundancy group that this network element
      belongs to. If the network element has not been placed into a redundant
      configuration, an empty(zero-length) string is returned for this object."
     ::= { bncElementEntry 8 }

   bncElementRedundancyRole OBJECT-TYPE
     SYNTAX      INTEGER { standAlone(0), primary(1), backup(2) }
     MAX-ACCESS  read-only
     STATUS      current
     DESCRIPTION "Redundancy role of the network element within system. If the
      network element has not been placed into a redundant configuration, the
      value standAlone is returned.
      standAlone  - no redundancy switching is provided for this network
       element.
      primary	- a redundant network element that is normally providing service.
      backup - a redundant network element that is switched into service in the
       event of a failure of a primary network element."
     ::= { bncElementEntry 9 }

   bncElementRequestedOpState OBJECT-TYPE	
     SYNTAX      INTEGER { online(0), offLine(1) }
     MAX-ACCESS  read-only
     STATUS      current
     DESCRIPTION "The expected operational state of a network element is
      represented by this object. Return online if network element is not a
      member of a redundancy group."
     ::= { bncElementEntry 10 }

   bncElementActualOpState OBJECT-TYPE	
     SYNTAX      INTEGER { active(0), standby(1), offLine(2) }
     MAX-ACCESS  read-only
     STATUS      current
     DESCRIPTION "The operational state of this network element as reported by
      the network element's redundancy controller. The actual operational state
      may differ from the expected operational state if the network element has
      faulted or cannot provide expected service.
      active  - providing service
      standby - not providing service, but able to provide service
      offLine - not providing service and/or unable to provide service."
     ::= { bncElementEntry 11 }

   bncElementHealthStatus OBJECT-TYPE
     SYNTAX      INTEGER { ok(0), unknown(1), warning(2), error(3), fatal(4) }
     MAX-ACCESS  read-only
     STATUS      current
     DESCRIPTION "Status of this network element.  At startup, this alarm status
      defaults to 'unknown' (1)."
     ::= { bncElementEntry 12 }

   bncElementHealthStatusLastChange OBJECT-TYPE
     SYNTAX      DateAndTime
     MAX-ACCESS  read-only
     STATUS      current
     DESCRIPTION "Time of the last change in the health status of the network
      element."
     ::= { bncElementEntry 13 }

   bncElementPositionID OBJECT-TYPE
     SYNTAX      DisplayString
     MAX-ACCESS  read-only
     STATUS      current
     DESCRIPTION "A network element identifier that is unique within a single
      BNC system and that can be used to identify similar network elements
      when doing A:B comparison of two BNC system configurations."
     ::= { bncElementEntry 14 }

   bncDatabase OBJECT IDENTIFIER ::= { bncSystem 53 }

   bncDatabaseConnections OBJECT-TYPE
     SYNTAX      Gauge32
     MAX-ACCESS  read-only
     STATUS      current
     DESCRIPTION "The current number of database connections."
   ::= { bncDatabase 1 }

   bncDatabaseImageSize OBJECT-TYPE
     SYNTAX      Integer32
     MAX-ACCESS  read-only
     STATUS      current
     DESCRIPTION "The database image size in kilobytes."
